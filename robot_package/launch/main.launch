<launch>

<include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find agro)/world/robot_control.world"/>
        <arg name="paused" value="false"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="gui" value="true"/>
        <arg name="recording" value="false"/>
        <arg name="debug" value="false"/>
        <arg name="verbose" value="true"/>
        <arg name="gui_required" value="true"/>
    </include>

  <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop_twist_keyboard" output="screen"/>

    
   

  <!-- <node name="test" pkg="agro" type="test.py" output="screen"/>
  <node name="robot" pkg="agro" type="robot.py" output="screen"/> -->
  <node name="robot" pkg="agro" type="imu_simulation.py" output="screen"/>

  <node name="vision_simulation" pkg="agro" type="vision_simulation.py" output="screen"/>
  
  <!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find agro)/configs.rviz"/> -->

</launch>
